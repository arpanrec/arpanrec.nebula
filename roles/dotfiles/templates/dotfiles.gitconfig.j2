[core]
	repositoryformatversion = 0
	filemode = true
	bare = true
[remote "origin"]
	url = {{ dotfiles_rv_git_remote }}
    fetch = +refs/heads/*:refs/remotes/origin/*
[status]
	showUntrackedFiles = no
[branch "{{ dotfiles_rv_git_version }}"]
	remote = origin
	merge = refs/heads/{{ dotfiles_rv_git_version }}
